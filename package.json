{
	"name": "taomu-routes",
	"version": "0.1.5",
	"private": false,
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./types": {
			"types": "./types/index.d.ts"
		}
	},
	"files": [
		"README.md",
		"LICENSE",
		"types",
		"dist"
	],
	"scripts": {
		"pu": "npm run test && npm run build && npm publish",
		"build": "node ./scripts/rm-dist.js && tsc --declaration & vite build",
		"dev": "vite --host",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:dev": "vitest",
		"build:docs": "typedoc"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-router-dom": ">=6"
	},
	"dependencies": {
		"taomu-toolkit": "^0.1.5"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.13",
		"@types/node": "^22.10.2",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"@vitest/ui": "^2.1.8",
		"react-router-dom": "^7.0.2",
		"typedoc": "^0.27.4",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vitest": "^2.1.8"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/taomu-org/taomu-routes"
	},
	"homepage": "https://taomu-org.github.io/taomu-routes/",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}