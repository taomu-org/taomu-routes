{
	"name": "taomu-routes",
	"version": "0.1.0",
	"private": false,
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./types": {
			"types": "./types/index.d.ts"
		}
	},
	"files": [
		"README.md",
		"types",
		"dist"
	],
	"scripts": {
		"pu": "npm run test && npm run build && npm publish",
		"build": "node ./scripts/rm-dist.js && tsc --declaration & vite build",
		"dev": "vite --host",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:dev": "vitest",
		"build:docs": "typedoc"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-router-dom": ">=6"
	},
	"dependencies": {
		"taomu-toolkit": "^0.0.9"
	},
	"devDependencies": {
		"@types/node": "^22.5.5",
		"@types/react": "^18.3.8",
		"@types/react-dom": "^18.3.0",
		"@vitest/ui": "^2.1.1",
		"react-router-dom": "^6.26.2",
		"typedoc": "^0.26.7",
		"typescript": "^5.6.2",
		"vite": "^5.4.7",
		"vitest": "^2.1.1"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	}
}